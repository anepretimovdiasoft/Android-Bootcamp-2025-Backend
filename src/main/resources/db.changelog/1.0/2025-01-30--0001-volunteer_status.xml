<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
                   http://www.liquibase.org/xml/ns/dbchangelog-ext  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">


    
    <changeSet id="2025-01-30--0001-volunteer_status" author="mabubakirova">
        <preConditions>
            <not>
                <tableExists tableName="volunteer_status"/>

            </not>
        </preConditions>
        <createTable tableName="volunteer_status">

            <column name = "id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="photo_url" type="VARCHAR(100)"/>
            <column name="isbusy" type="BOOL">
                <constraints nullable="false"/>
            </column>

        </createTable>

    </changeSet>
</databaseChangeLog>